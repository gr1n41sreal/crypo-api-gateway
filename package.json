{
  "name": "api-gateway",
  "version": "1.0.0",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:proto": "prettier --write \"src/**/*.pb.ts\"",
    "proto:generate": "protoc --plugin=node_modules/.bin/protoc-gen-ts_proto -I=./src/_proto --ts_proto_out=src src/_proto/*/*.proto --ts_proto_opt=nestJs=true --ts_proto_opt=unrecognizedEnum=false --ts_proto_opt=removeEnumPrefix=true --ts_proto_opt=fileSuffix=.pb --ts_proto_opt=addGrpcMetadata=true --ts_proto_opt=useOptionals=all --ts_proto_opt=unrecognizedEnum=false && npm run format:proto",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@fastify/compress": "^6.4.0",
    "@fastify/helmet": "^11.0.0",
    "@fastify/multipart": "^7.7.0",
    "@fastify/static": "^6.10.2",
    "@grpc/grpc-js": "^1.8.2",
    "@grpc/proto-loader": "^0.7.4",
    "@nestjs/axios": "^3.0.3",
    "@nestjs/common": "^10.0.5",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.5",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/microservices": "^10.0.5",
    "@nestjs/platform-fastify": "^10.0.5",
    "@nestjs/swagger": "^7.1.1",
    "@nestjs/throttler": "^4.1.0",
    "axios": "^1.7.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "nestjs-pino": "^3.1.2",
    "pino": "^8.8.0",
    "pino-http": "^8.3.1",
    "pino-pretty": "^9.1.1",
    "reflect-metadata": "^0.2.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "prettier": "^2.8.2",
    "ts-proto": "^1.162.1",
    "typescript": "^5.6.2"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
